<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#FFFFFF"
    tools:ignore="ContentDescription">
<TextView
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:text="校园"
        android:id="@+id/textView"
        android:gravity="center"
        android:textColor="#FFFFFF"
        android:background="#004c92"
        android:textSize="22dp"/>



    <RelativeLayout
        android:layout_width="320dp"
        android:layout_height="300dp"
        android:background="@drawable/pageone"
        android:id="@+id/relativeLayout"
        android:layout_below="@+id/frameLayout"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="学校概况"
            android:textColor="#080808"
            android:layout_marginLeft="30dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="61dp"
            android:id="@+id/textView3"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="院系机构"
            android:textColor="#080808"
            android:layout_alignTop="@+id/textView3"
            android:layout_centerHorizontal="true"
            android:id="@+id/textView4" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="长职要闻"
            android:textColor="#080808"
            android:layout_marginRight="29dp"
            android:layout_marginEnd="29dp"
            android:id="@+id/textView7"
            android:layout_alignTop="@+id/textView4"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="校园公告"
            android:textColor="#080808"
            android:id="@+id/textView5"
            android:layout_marginBottom="82dp"
            android:layout_above="@+id/textView8"
            android:layout_alignLeft="@+id/textView3"
            android:layout_alignStart="@+id/textView3" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="媒体长职"
            android:textColor="#080808"
            android:id="@+id/textView2"
            android:layout_alignTop="@+id/textView5"
            android:layout_alignRight="@+id/textView4"
            android:layout_alignEnd="@+id/textView4" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="数字校园"
            android:textColor="#080808"
            android:id="@+id/textView6"
            android:layout_alignTop="@+id/textView2"
            android:layout_alignLeft="@+id/textView7"
            android:layout_alignStart="@+id/textView7" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="人文讲座"
            android:id="@+id/textView8"
            android:textColor="#080808"
            android:layout_marginBottom="24dp"
            android:layout_alignParentBottom="true"
            android:layout_alignLeft="@+id/textView5"
            android:layout_alignStart="@+id/textView5" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="校园媒体"
            android:textColor="#080808"
            android:id="@+id/textView9"
            android:layout_alignTop="@+id/textView8"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="校历查询"
            android:textColor="#080808"
            android:id="@+id/textView10"
            android:layout_alignTop="@+id/textView9"
            android:layout_alignLeft="@+id/textView6"
            android:layout_alignStart="@+id/textView6" />

        <ImageButton
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:id="@+id/imageButton1"
            android:src="@drawable/button"
            android:background="@drawable/a1"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:id="@+id/imageButton3"
            android:src="@drawable/button"
            android:background="@drawable/c1"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageButton
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:id="@+id/imageButton2"
            android:src="@drawable/button"
            android:background="@drawable/b1"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" />

        <ImageButton
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:id="@+id/imageButton4"
            android:src="@drawable/button"
            android:background="@drawable/d1"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/imageButton2"
            android:layout_toStartOf="@+id/imageButton2" />

        <ImageButton
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:id="@+id/imageButton5"
            android:src="@drawable/button"
            android:background="@drawable/e1"
            android:layout_alignTop="@+id/imageButton4"
            android:layout_toRightOf="@+id/imageButton4"
            android:layout_toEndOf="@+id/imageButton4" />

        <ImageButton
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:id="@+id/imageButton6"
            android:src="@drawable/button"
            android:background="@drawable/f1"
            android:layout_alignTop="@+id/imageButton5"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageButton
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:id="@+id/imageButton7"
            android:src="@drawable/button"
            android:background="@drawable/g1"
            android:layout_below="@+id/imageButton4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:id="@+id/imageButton9"
            android:src="@drawable/button"
            android:background="@drawable/i1"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageButton
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:id="@+id/imageButton8"
            android:src="@drawable/button"
            android:background="@drawable/h1"
            android:layout_below="@+id/imageButton4"
            android:layout_toRightOf="@+id/imageButton4"
            android:layout_toEndOf="@+id/imageButton4" />

    </RelativeLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bk"
        android:layout_toLeftOf="@+id/relativeLayout"
        android:layout_toStartOf="@+id/relativeLayout"
        android:layout_alignBottom="@+id/relativeLayout"
        android:layout_alignTop="@+id/relativeLayout">
    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="215dp"
        android:layout_below="@+id/textView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/frameLayout">

        <WebView
            android:layout_width="match_parent"
            android:layout_height="215dp"
            android:id="@+id/webView1"
            android:layout_gravity="center_horizontal|top" />
    </FrameLayout>


</RelativeLayout>